<div class="header">
<center></center>
 <div class="container">
   
   <center><h1>Master Project List</h>
   <h2><%= link_to "Add New Project", 'projects/new' %>    <%=link_to"Project Page", 'projects#index' %>    <%= link_to "Main Menu", '/' %></h2></center>
 </div>
</div>

<div class="projectslist">
 <center><div class="container">

 <table border = 1>
 	<tr>
 		<td><h3>ID</h3></td>
 		<td><h3>Project Name</h3></td>
 		<td><h3>Client</h3></td>
 		<td><h3>Programmer</h3></td>
 		<td><h3>Action</h3></td>
 	</tr>
  <% @projects.each do |f| %>
  	<tr>
		<td><%= f.id %></td>
		<td><%= f.project_name %></td>
		<td><%= f.client_id %></td>
		<td><%= f.programmer_id %></td>
		<td><%= link_to "Edit", edit_project_path(f) %>  <%= link_to "Delete", project_path(f), method: :delete, data: {confirm: "Are you sure?" } %></td>
	</tr>
  
<% end %>
		</center>
 </div>
</div>